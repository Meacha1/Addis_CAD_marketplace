{
  "version": 2,
  "builds": [
    {
      "src": "cad_market/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build/static/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "cad_market/wsgi.py"
    }
  ]
}
