{
  "version": 3,
  "builds": [
    {
      "src": "cad_market/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ""
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "cad_market/wsgi.py"
    }
  ],
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/build/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/cad_market/wsgi.py"
    }
  ]
}
